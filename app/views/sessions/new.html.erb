<h1>Sign In</h1>

<%= form_with url: sign_in_path , local: true do |form| %>
	<div>
		<%= form.label :email %>
		<%= form.text_field :email %>
	</div>
	<div>
		<%= form.label :password %>
		<%= form.password_field :password %>
	</div>
	<div>
		<%= link_to 'SIGN UP', sign_up_path, class: 'btn btn-default' %>
	</div>
	<div>
		<%= form.submit 'SIGN IN' %>
	</div>
<% end %>